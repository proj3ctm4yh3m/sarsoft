<ehcache>
	<diskStore path="java.io.tmpdir"/>
	<defaultCache
		maxElementsInMemory="5000"
		eternal="false"
		timeToIdleSeconds="172800"
		timeToLiveSeconds="172800"
		overflowToDisk="true"
		diskPersistent="true"
		memoryStoreEvictionPolicy="LRU"/>
		
	<cache name="tileCache"/>			
		
</ehcache>